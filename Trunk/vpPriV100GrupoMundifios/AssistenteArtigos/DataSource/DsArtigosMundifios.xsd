                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Dados.Empresa, 
Dados.ArmazemSugestao , Dados.ArmazemSugestaoDescricao , 
Dados.TipoArtigo , Dados.TipoArtigoDescricao , 
Dados.TipoComponente , 
Dados.TipoComponenteDescricao , 
Dados.GrupoTaxaDesperdicio, GrupoTaxaDesperdiciodescricao , GrupoTaxaDesperdicioTaxa ,
Dados.IntrastatPautal , Dados.IntrastatPautalDescricao , 
case Dados.Empresa
	WHEN 'MUNDIFIOS' THEN COALESCE(Dados.CDU_CodigoAntigoMundifios,'')
	WHEN 'AVEFIOS'   THEN COALESCE(Dados.CDU_CodigoAntigoAvefios,'')
	WHEN 'INOVAFIL'  THEN COALESCE(Dados.CDU_CodigoAntigoInovafil,'')
	WHEN 'YARNTRADE' THEN COALESCE(Dados.CDU_CodigoAntigoYarntrade,'')
	WHEN 'FILOPA'	 THEN COALESCE(Dados.CDU_CodigoAntigoFilopa,'')
	else  '' END AS CodigoAntigo, 
case Dados.Empresa
	WHEN 'MUNDIFIOS' THEN COALESCE(AAntigoMundifios.Descricao,'')
	WHEN 'AVEFIOS'   THEN COALESCE(AAntigoAvefios.Descricao,'')
	WHEN 'INOVAFIL'  THEN COALESCE(AAntigoInovafil.Descricao,'')
	WHEN 'YARNTRADE' THEN COALESCE(AAntigoYarntrade.Descricao,'')
	WHEN 'FILOPA'	 THEN COALESCE(AAntigoFilopa.Descricao,'')
	else '' END AS CodigoAntigoDescricao
 FROM (

		SELECT  (SELECT TOP 1 Codigo  FROM TMP_Empresas) AS Empresa,  
		COALESCE(A.ArmazemSugestao,'') AS ArmazemSugestao , COALESCE(AR.Descricao,'')  AS ArmazemSugestaoDescricao,  
		A.TipoArtigo, TA.Descricao as TipoArtigoDescricao, 
		TipoComponente, 
		CASE A.TipoComponente WHEN  0 THEN 'Artigo Simples' WHEN 1 THEN 'Conjunto de Artigos' WHEN 2 THEN 'Artigo Composto' ELSE 'Artigo Simples' END AS TipoComponenteDescricao, 
		COALESCE(A.CDU_GrupoTaxaDesperdicio,'') as GrupoTaxaDesperdicio, COALESCE(GTD.cdu_descricao,'') AS GrupoTaxaDesperdicioDescricao, COALESCE(GTD.cdu_taxa,0) AS  GrupoTaxaDesperdicioTaxa,
		COALESCE(A.IntrastatPautal,'') AS IntrastatPautal, COALESCE(IM.Descricao,'') as IntrastatPautalDescricao ,
		CDU_CodigoAntigoMundifios , CDU_CodigoAntigoAvefios ,  CDU_CodigoAntigoInovafil , CDU_CodigoAntigoYarntrade ,CDU_CodigoAntigoFilopa
		FROM Artigo  A
		LEFT OUTER JOIN Armazens  AR ON AR.Armazem = A.ArmazemSugestao
		LEFT OUTER JOIN TiposArtigo TA ON TA.TipoArtigo = A.TipoArtigo
		LEFT OUTER JOIN TDU_GrupoTaxaDesperdicio GTD ON GTD.CDU_Codigo = A.CDU_GrupoTaxaDesperdicio
		LEFT OUTER JOIN IntrastatMercadoria IM ON IM.Mercadoria = A.IntrastatPautal
		WHERE A.Artigo = @Artigo
		) as Dados
	LEFT OUTER JOIN Artigo AAntigoMundifios ON AAntigoMundifios.Artigo = Dados.CDU_CodigoAntigoMundifios
	LEFT OUTER JOIN Artigo AAntigoAvefios ON AAntigoAvefios.Artigo = Dados.CDU_CodigoAntigoAvefios
	LEFT OUTER JOIN Artigo AAntigoInovafil ON AAntigoInovafil.Artigo = Dados.CDU_CodigoAntigoInovafil
	LEFT OUTER JOIN Artigo AAntigoYarntrade ON AAntigoYarntrade.Artigo = Dados.CDU_CodigoAntigoYARNTRADE
	LEFT OUTER JOIN Artigo AAntigoFilopa ON AAntigoFilopa.Artigo = Dados.CDU_CodigoAntigoFilopa</CommandText>
